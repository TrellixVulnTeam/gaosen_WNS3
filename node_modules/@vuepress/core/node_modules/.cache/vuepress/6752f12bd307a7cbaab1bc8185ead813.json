Interval","$","methods","removeDom","addDom","solveBg","typingEffect","i","p","document","delay","span","setTimeout","solveTypingEffect"],"sourceRoot":"docs/.vuepress/components","sources":["Temp.vue"],"sourcesContent":["<template>\r\n\r\n</template>\r\n\r\n<script>\r\nimport $ from 'jquery';\r\nexport default {\r\n  data() {\r\n    return {\r\n      welcomeStr: [\r\n        /*单个元素不要超过12+3=15字*/\r\n        '忆已往兮,宠辱偕忘,心旷神怡.',\r\n        '俟其离兮,思愁千万,心寓感激.',\r\n        '悟已往之不谏,知来者之可追。',\r\n        '实迷途其未远，觉今是而昨非。',\r\n        '当时明月在，曾照彩云归。',\r\n        '世人慌慌张张',\r\n        '不过图银碎几两',\r\n        '偏偏这银碎几两',\r\n        '能解万千惆怅',\r\n        '人们都相信',\r\n        '别人是单纯的坏人',\r\n        '自己是复杂的好人',\r\n        '..............'\r\n      ],\r\n      typingFlag: 0\r\n    }\r\n  },\r\n  mounted() {\r\n    this.solveBg();\r\n    /*周期执行生成/删除特效文字*/\r\n    if(this.$page.frontmatter.home) {\r\n      setInterval(() => {\r\n        console.log(this.$page.frontmatter.home);\r\n        if(!this.$page.frontmatter.home) {\r\n          clearInterval();\r\n          console.log('notHome');\r\n          $('div.root').remove();\r\n        }\r\n        this.typingEffect(this.typingFlag);\r\n        this.typingFlag ++;\r\n        if(this.typingFlag >= this.welcomeStr.length) this.typingFlag = 0;\r\n      },4100);//这里的数字是周期\r\n    }\r\n  },\r\n  methods: {\r\n    removeDom() {\r\n      $('div.home-blog').remove();\r\n\r\n      /*删除特效文字,避免重复添加*/\r\n      $('div.root').remove();\r\n      console.log('remove');\r\n    },\r\n    addDom() {\r\n      /*添加动态壁纸*/\r\n      $('div.home-blog').prepend(`\r\n          <div class=\"bg-box\">\r\n            <video src=\"/img/bg0.webm\" autoplay muted loop width=\"100%\"></video>\r\n            <div class=\"text-box\">\r\n              <p class=\"p1\">GaoSen</p>\r\n              <p class=\"p2\">海压竹枝低复举，风吹山角晦还明。</p>\r\n            </div>\r\n          </div>\r\n        `);\r\n      console.log('add');\r\n    },\r\n    solveBg() {\r\n      /*解决动态壁纸问题*/\r\n      this.addDom();\r\n      $('div.hero').remove();\r\n      $('div.page-title').remove();\r\n      $('div.theme-reco-content').removeClass('theme-reco-content content__default');\r\n      $('main.page').addClass('x');\r\n      $('footer.page-edit').remove();\r\n      console.log('mounted');\r\n    },\r\n    typingEffect(i) {\r\n      let welcomeStr = this.welcomeStr;\r\n      /*添加特效文字*/\r\n      $('div.bg-box').append(`\r\n          <div class=\"root\">\r\n            <p class=\"text\">${welcomeStr[i]}</p>\r\n          </div>\r\n        `);\r\n\r\n      console.log('typingFn');\r\n      const p = document.querySelector('p.text');\r\n      p.innerHTML = p.textContent.replace(/\\S/g,\"<span>$&</span>\");\r\n\r\n      let delay = 0;\r\n      document.querySelectorAll('.text span').forEach((span, index) => {\r\n        delay += 0.1;\r\n        if(index %5 === 0) delay += 0.3;\r\n        span.style.setProperty('--delay', `${delay}s`)\r\n      });\r\n\r\n      p.addEventListener('animationend', (e) => {\r\n        if(e.target === document.querySelector('p.text span:last-child')) {\r\n          p.classList.add('ended');\r\n          setTimeout(() => {\r\n            $('div.root').remove()\r\n          },1500)//这里的数字是最后一个字符的停顿时间\r\n        }\r\n      })\r\n    },\r\n    solveTypingEffect() {\r\n\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"]}]}