,CAAuB,oCAAvB,CADhB,EAEE;AACA,aAAKH,EAAL,CAAQ8B,SAAR,CAAkBC,GAAlB,CAAsB,OAAtB;AACAC,QAAAA,UAAU,CAAC,YAAM;AACf,UAAA,MAAI,CAAChC,EAAL,CAAQiC,mBAAR,CAA4B,cAA5B,EAA4C,MAAI,CAACN,OAAjD;;AACA,cAAIX,KAAK,GAAG,CAAZ;;AAEA,UAAA,MAAI,CAACjB,KAAL,CAAWmC,OAAX,GAAqBhB,OAArB,CAA6B,UAACC,IAAD,EAAOC,CAAP,EAAa;AACxC,YAAA,MAAI,CAACpB,EAAL,CAAQ8B,SAAR,CAAkBK,MAAlB,CAAyB,OAAzB;;AACAhB,YAAAA,IAAI,CAACK,KAAL,CAAWY,KAAX,GAAmB,KAAnB;AACAjB,YAAAA,IAAI,CAACK,KAAL,CAAWa,SAAX,GAAuB,oCAAvB;AACArB,YAAAA,KAAK,IAAI,IAAT,CAJwC,CAMxC;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEAG,YAAAA,IAAI,CAACK,KAAL,CAAWc,cAAX,aAA+BtB,KAA/B;AACD,WA3BD;;AA4BA,UAAA,MAAI,CAAChB,EAAL,CAAQ0B,gBAAR,CAAyB,cAAzB,EAAyC,MAAI,CAACa,QAA9C;AACD,SAjCS,EAiCP,IAjCO,CAAV;AAkCD;AACF,KAvEM;AAyEPA,IAAAA,QAzEO,oBAyEEX,CAzEF,EAyEK;AACV,UACIA,CAAC,CAACC,MAAF,IACA3B,QAAQ,CAACC,aAAT,CAAuB,qCAAvB,CAFJ,EAGE;AACA,aAAKJ,KAAL,CAAWmB,OAAX,CAAmB,UAACsB,CAAD,EAAO;AACxBA,UAAAA,CAAC,CAACL,MAAF;AACD,SAFD;AAGA,aAAKnC,EAAL,CAAQiC,mBAAR,CAA4B,cAA5B,EAA4C,KAAKM,QAAjD;AACA,aAAKzC,YAAL;AACA,aAAKU,IAAL;AACD;AACF;AArFM;AAnEI,CAAf","sourcesContent":["//\n//\n//\n//\n\r\nexport default {\r\n  name: \"\",\r\n  data() {\r\n    return {\r\n      strs: [\r\n        \"发现光\",\r\n        \"跟随光\",\r\n        \"靠近光\",\r\n        \"成为光\",\r\n        \"散发光\",\r\n        {\r\n          title: \"愿你我都能,明确的爱\",\r\n          // 停顿的位置\r\n          stop: 6,\r\n          // stop: [4, 13] // 可以是数组，多几个位置停顿\r\n        },\r\n        \"真诚的喜欢\",\r\n        \"直接的厌恶\",\r\n        \"站在太阳下的坦荡\",\r\n        \"还有被坚定的选择!\",\r\n        {\r\n          title: \"有一份热，发一份光\",\r\n          // 停顿的位置\r\n          stop: 5,\r\n          // stop: [4, 13] // 可以是数组，多几个位置停顿\r\n        },\r\n        {\r\n          title: \"就如萤火一般\",\r\n        },\r\n        {\r\n          title: \"也可以在黑暗里发一点光\",\r\n        },\r\n        {\r\n          title: \"不必等候炬火\",\r\n        },\r\n        {\r\n          title: \"此后如竟没有炬火\",\r\n        },\r\n        {\r\n          title: \"我便是唯一的光\",\r\n        },\r\n        {\r\n          title: \"愿与所有中国青年共勉\",\r\n        },\r\n      ],\r\n      // 当前进行到第几行\r\n      currentIndex: 0,\r\n      spans: null,\r\n      el: null,\r\n    };\r\n  },\r\n  mounted() {\r\n    this.el = document.querySelector(\".hero .description\");\r\n\r\n    if (!this.el) {\r\n      let that = this;\r\n      const timer = setInterval(() => {\r\n        if (this.el) {\r\n          timer && clearInterval(timer);\r\n          that.init();\r\n        }\r\n        this.el = document.querySelector(\".hero .description\");\r\n      }, 100);\r\n    } else {\r\n      this.init();\r\n    }\r\n  },\r\n  methods: {\r\n    init() {\r\n      if (this.currentIndex == this.strs.length) {\r\n        this.currentIndex = 0;\r\n      }\r\n      const current = this.strs[this.currentIndex];\r\n      this.el.innerHTML = current instanceof Object ? current.title : current;\r\n      this.el.innerHTML = this.el.textContent.replace(/\\S/g, \"<span>$&</span>\");\r\n\r\n      let delay = 0;\r\n      this.spans = [...document.querySelectorAll(\".hero .description span\")];\r\n      this.spans.forEach((span, i) => {\r\n        delay += 0.1;\r\n        if (current.hasOwnProperty(\"stop\")) {\r\n          if (current.stop instanceof Array) {\r\n            if (current.stop.includes(i)) {\r\n              delay += 0.3;\r\n            }\r\n          } else {\r\n            if (current.stop == i) {\r\n              delay += 0.3;\r\n            }\r\n          }\r\n        }\r\n\r\n        span.style.setProperty(\"--delay\", `${delay}s`);\r\n      });\r\n\r\n      this.el.addEventListener(\"animationend\", this.lastEnd);\r\n    },\r\n\r\n    lastEnd(e) {\r\n      if (\r\n          e.target == document.querySelector(\".hero .description span:last-child\")\r\n      ) {\r\n        this.el.classList.add(\"ended\");\r\n        setTimeout(() => {\r\n          this.el.removeEventListener(\"animationend\", this.lastEnd);\r\n          let delay = 0;\r\n\r\n          this.spans.reverse().forEach((span, i) => {\r\n            this.el.classList.remove(\"ended\");\r\n            span.style.width = \"2ch\";\r\n            span.style.animation = \"0.1s text-out ease-in-out forwards\";\r\n            delay += 0.05;\r\n\r\n            // 回去停顿功能\r\n            // if (this.strs[this.currentIndex].hasOwnProperty(\"stop\")) {\r\n            //   if (this.strs[this.currentIndex].stop instanceof Array) {\r\n            //     if (\r\n            //       this.strs[this.currentIndex].stop.includes(\r\n            //         this.spans.length - i\r\n            //       )\r\n            //     ) {\r\n            //       delay += 0.3;\r\n            //     }\r\n            //   } else {\r\n            //     if (\r\n            //       this.strs[this.currentIndex].stop ==\r\n            //       this.spans.length - i\r\n            //     ) {\r\n            //       delay += 0.3;\r\n            //     }\r\n            //   }\r\n            // }\r\n\r\n            span.style.animationDelay = `${delay}s`;\r\n          });\r\n          this.el.addEventListener(\"animationend\", this.firstEnd);\r\n        }, 1500);\r\n      }\r\n    },\r\n\r\n    firstEnd(e) {\r\n      if (\r\n          e.target ==\r\n          document.querySelector(\".hero .description span:first-child\")\r\n      ) {\r\n        this.spans.forEach((v) => {\r\n          v.remove();\r\n        });\r\n        this.el.removeEventListener(\"animationend\", this.firstEnd);\r\n        this.currentIndex++;\r\n        this.init();\r\n      }\r\n    },\r\n  },\r\n};\r\n"]}]}