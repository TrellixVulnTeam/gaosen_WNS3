ray)\r\n  },\r\n  methods: {\r\n    IN_BOARD(sq) {\r\n      return this.IN_BOARD_[sq] != 0;\r\n    },\r\n    FENtoBoard(FENArray) {// 将FEN串转为一维数组\r\n      // console.log(FENArray.length);\r\n      // console.log(FENArray[5]);\r\n      let x = 3, y = 3\r\n      for (let i in FENArray) {\r\n        let ch = FENArray[i]\r\n        if (y > 12) break\r\n        if (ch === '/') {//换行\r\n          y++\r\n        }\r\n        else if (ch >= '1' && ch <= '9') {//出现空位\r\n          x += parseInt(ch)\r\n        }\r\n        else if ((ch >= 'A' && ch <= 'Z') || (ch >= 'a' && ch <= 'z')) {//红方棋子或黑方棋子\r\n          let index = x + (y << 4)\r\n          this.displayChess[index] = this.FENCharToNum(ch)\r\n        }\r\n        else console.log('err');\r\n      }\r\n    },\r\n    FENCharToNum(item) {//将棋子字符转成对应的整数\r\n      switch (item) {\r\n        case 'K' : {\r\n          return 8\r\n        }\r\n        case 'A':\r\n          return 9;\r\n        case 'B':\r\n          return 10;\r\n        case 'N':\r\n          return 11;\r\n        case 'R':\r\n          return 12;\r\n        case 'C':\r\n          return 13;\r\n        case 'P':\r\n          return 14;\r\n\r\n        case 'k':\r\n          return 16;\r\n        case 'a':\r\n          return 17;\r\n        case 'b':\r\n          return 18;\r\n        case 'n':\r\n          return 19;\r\n        case 'r':\r\n          return 20;\r\n        case 'c':\r\n          return 21;\r\n        case 'p':\r\n          return 22;\r\n      }\r\n    }\r\n  },\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"]}]}