tener('animationend', (e) => {\r\n          if (e.target === document.querySelector('p.text span:last-child')) {\r\n            p.classList.add('ended')\r\n          }\r\n        })\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n.root {\r\n  font-family: 'my-font';\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  //min-height: 100vh;\r\n  height: 20%;\r\n  position: absolute;\r\n  top: 38vw;\r\n  z-index: 4;\r\n  left: 50%;\r\n  transform: translateX(-50%);\r\n  width: 100%;\r\n  .text {\r\n    font-size: 3vw;\r\n    margin: 0;\r\n    padding: 0;\r\n    //font-family: monospace;\r\n    position: relative;\r\n    line-height: normal;\r\n    color: #0dcac8;\r\n    span {\r\n      --delay: 10s;\r\n\r\n      display: inline-block;\r\n      overflow: hidden;\r\n      width: 0ch;\r\n      animation: 0.1s text-in ease-in-out forwards;\r\n      animation-delay: var(--delay);\r\n    }\r\n    @keyframes cursor {\r\n      /*光标闪烁*/\r\n      from {\r\n        opacity: 0;\r\n      }\r\n      to {\r\n        opacity: 1;\r\n      }\r\n    }\r\n\r\n    @keyframes text-in {\r\n      from {\r\n        width: 0ch;\r\n      }\r\n      to {\r\n        width: 2ch;\r\n      }\r\n    }\r\n  }\r\n\r\n  .text::after {\r\n    content: '';\r\n    display: inline-block;\r\n    position: absolute;\r\n    width: 0.5vw;\r\n    height: 3vw;\r\n    background-color: #0dcac8;\r\n    border-radius: 2px;\r\n    right: -1vw;\r\n    top: 0.2vw;\r\n    /*一直闪烁*/\r\n    animation: 1.1s cursor steps(2, jump-none) infinite;\r\n  }\r\n\r\n  .text.ended::after {\r\n    /*光字体显示全部后光标才闪烁*/\r\n    //animation: 1.1s cursor steps(2, jump-none) infinite;\r\n  }\r\n\r\n}\r\n\r\n@font-face {\r\n  font-family: 'my-font';\r\n  //src: url('YourWebFontName.eot'); /* IE9 Compat Modes */\r\n  //src: url('YourWebFontName.eot?#iefix') format('embedded-opentype'), /* IE6-IE8 */\r\n  url('/css/myfont.woff') format('woff'), /* Modern Browsers */\r\n  url('/css/my-font.ttf')  format('truetype'), /* Safari, Android, iOS */\r\n  url('my-font.svg#my-font') format('svg'); /* Legacy iOS */\r\n}\r\n</style>"]}]}